    <link rel="stylesheet" href="../../bank/assets/css/animated.css" />
    <link rel="stylesheet" href="../../bank/assets/css/owl.css" />
  </head>

  <body>
    <main>
      <!-- ***** Header Area Start ***** -->
      <header id="header">
        <nav>
          <div class="container">
            <div class="logo">
              <a href="/">
                <img src="../../bank/img/logo (2).png" alt="" />
              </a>
            </div>
            <div>
              <input type="checkbox" class="checkbox" id="chk" />
              <label class="label" for="chk">
                <i class="fas fa-moon"></i>
                <i class="fas fa-sun"></i>
                <div class="ball"></div>
              </label>
            </div>
          </div>
        </nav>
      </header>

      <!-- ***** Header Area End ***** -->

      <div id="services" class="our-services section">
        <div class="container">
          <div class="row">
            <div class="col-lg-6 offset-lg-3">
              <div
                class="section-heading wow bounceIn"
                data-wow-duration="1s"
                data-wow-delay="0.2s"
              >
                <h6>Your</h6>
                <h2>Question Bank</h2>
              </div>
            </div>
          </div>
        </div>
        <div class="container-fluid">
          <div class="row">
            <% for(i of data){ %> 
              <% if(i){%>
            <div class="col-lg-4">
              <div
                class="service-item wow bounceInUp"
                data-wow-duration="1s"
                data-wow-delay="0.3s"
              >

                <div class="row">
                  <div class="col-lg-4">
                    <div class="icon">
                      <i class="fa fa-user"></i>
                    </div>
                    <p class="exercise">
                      Download
                      <% let pdf = i['pdf'].split('\\')[1] || i['pdf'].split('/')[1] %>

                      <a rel="nofollow" href="/<%=pdf%>">Pdf</a>
                    </p>
                  </div>

                  <div class="col-lg-8">
                    <div class="right-content">
                      <h4><%=i.namePdf%></h4>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <%} %> 
            <% } %> 
          </div>
        </div>
      </div>
      <br />
    </main>

    <footer class="footer">
      <div class="container">
        <div class="grid-4"></div>

        <div class="bottom-footer">
          <div class="copyright">
            <p class="text">
              Â© Copyright 2022. All Rights Reserved.

